#include<stdio.h>
#include<stdlib.h>
#include<time.h>



int main(){
	int mat[3][3];
	int resta[3][3];
	int mat1[3][3];
	int aleatorios = 1+rand()%(10-1);
	
	printf ("PRIMER MATRIZ: \n");
	for(int k = 0; k < 3; k ++){
 		for (int l = 0; l < 3; l ++){
 			mat[k][l] = aleatorios;
 			
		 }
	 }
	 
	printf ("\nSEGUNDA MATRIZ: \n");
	for(int k = 0; k < 3; k ++){
 		for (int l = 0; l < 3; l ++){
 			mat1[k][l] = aleatorios;
 			printf("[%i]", mat1[k][l]);
		 }
	 }
	 
	for (int i = 0; i < 3; i ++){
		for (int j = 0; j < 3; j ++){
			resta [0][0] = mat[0][0] - mat1 [0][0];
			resta [1][1] = mat[1][1] - mat1 [1][1];
			resta [1][1] = mat[1][1] - mat1 [1][1];
		}
	}
	printf ("\nRESTA DIAGONAL MATRICES: \n");
	for (int i = 0; i < 3; i ++){
		for (int j = 0; j < 3; j ++){
			printf("[%i]", resta[i][j]);
		}
		printf("\n");
	}
	
